  <menu id="openbox-settings" label="Openbox">
    <menu id="Obmenu-Generator" label="Obmenu-Generator">
      <item label="Menu Schema">
        <action name="Execute">
          <command>geany ~/.config/obmenu-generator/schema.pl</command>
        </action>
      </item>
      <item label="Menu Config">
        <action name="Execute">
          <command>geany ~/.config/obmenu-generator/config.pl</command>
        </action>
      </item>
      <separator/>
      <item label="Generate a pipe menu">
        <action name="Execute">
          <command>obmenu-generator -p</command>
        </action>
      </item>
      <item label="Generate a static menu">
        <action name="Execute">
          <command>obmenu-generator -s -c</command>
        </action>
      </item>
      <item label="Generate a pipe menu with icons">
        <action name="Execute">
          <command>obmenu-generator -p -i</command>
        </action>
      </item>
      <item label="Generate a static menu with icons">
        <action name="Execute">
          <command>obmenu-generator -s -i -c</command>
        </action>
      </item>
      <separator/>
      <item label="Refresh Icon Set">
        <action name="Execute">
          <command>obmenu-generator -d</command>
        </action>
      </item>
    </menu>
  </menu>
